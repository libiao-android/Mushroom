<?xml version="1.0" encoding="utf-8"?>
<com.libiao.mushroom.mine.TimeItemView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_marginBottom="5dp"
    android:background="#f1f1f1"
    android:layout_height="35dp">
    <TextView
        android:layout_marginLeft="6dp"
        android:textColor="@color/blank"
        android:textSize="16dp"
        android:layout_centerVertical="true"
        android:id="@+id/time_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <TextView
        android:layout_toRightOf="@+id/time_item"
        android:layout_marginLeft="10dp"
        android:textColor="@color/blank"
        android:textSize="16dp"
        android:layout_centerVertical="true"
        android:id="@+id/time_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <ImageView
        android:src="@mipmap/icon_green_arrow_up"
        android:layout_centerVertical="true"
        android:layout_marginRight="10dp"
        android:id="@+id/iv_time_expand"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"/>

</com.libiao.mushroom.mine.TimeItemView>